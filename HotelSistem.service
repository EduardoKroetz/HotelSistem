[Unit]
Description=Hotel Sistem API
After=network.target

[Service]
WorkingDirectory=/var/www/hotelsistem/Hotel.Domain
ExecStart=/usr/bin/dotnet /var/www/hotelsistem/Hotel.Domain/Hotel.Domain.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=hotelsistem
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Production
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false

[Install]
WantedBy=multi-user.target
